# drawOn: aCanvas
#   (Array with: self horBar with: self verBar) do: [ :rect |
#     aCanvas fillRectangle: rect color: self color.
#     ]
#
self=$1
aCanvas=$2
tmp1=$(./send $self horBar)
tmp2=$(./send $self verBar)
tmp3=$(./send Array with-with- $tmp1 $tmp2)
tmp4=$(./send Array with-with- $self $aCanvas)
blockDir=${0%/*}
tmp5=$(./send BlockClosure fromCode-with- $blockDir/drawOn-~block1 $tmp4)
./send $tmp3 do- $tmp5
