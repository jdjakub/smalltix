# _method: self and: aClass and: elem
# (elem isMemberOf: aClass) ifTrue: [^true]
#
self=$1
aClass=$2
elem=$3
tmp1=$(./send $elem isMemberOf- $aClass)
tmp2=$(./send Array with-with-with- $self $elem $aClass)
blockDir=${0%/*}
tmp3=$(./send BlockClosure fromCode-with- $blockDir/containsInstanceOf-~block1~block2 $tmp2)
./send $tmp1 ifTrue- $tmp3
